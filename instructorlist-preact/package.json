{
  "private": true,
  "name": "instructorlist",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "find . -name '.DS_Store' -type f -delete && ./node_modules/.bin/preact build --json --template template.ejs && rm -rf ../instructorlist-express/frontend-build-copy/* && cp -r ./build/* ../instructorlist-express/frontend-build-copy/",
    "serve": "./node_modules/.bin/preact build --json --template template.ejs && ./node_modules/.bin/preact serve",
    "dev": "./node_modules/.bin/preact watch --https --template template.ejs ",
    "preact-version": "./node_modules/.bin/preact -v",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.2",
    "babel-eslint": "^10.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-config-synacor": "^2.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.12.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-next.1",
    "preact-cli-plugin-async": "^2.0.0",
    "preact-cli-plugin-critical-css": "^0.1.0",
    "preact-render-spy": "^1.2.1",
    "sass-loader": "^7.2.0",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "dependencies": {
    "dayjs": "^1.8.15",
    "loadjs": "^3.6.1",
    "mapbox-gl": "^1.3.2",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-render-to-string": "^4.1.0",
    "preact-router": "^2.5.7",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js"
    ],
    "testRegex": "(/(__tests__|tests)/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/__mocks__/*"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^create-react-class$": "preact-compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  }
}
